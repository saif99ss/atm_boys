<p id="notice"><%= notice %></p>

<h1>Listing Atm Machines</h1>

<table class="table">
  <thead>
    <tr>
      <th>Address</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Name</th>
      <th>Street</th>
      <th>City</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @atm_machines.each do |atm_machine| %>
      <tr>
        <td><%= atm_machine.address %></td>
        <td><%= atm_machine.latitude %></td>
        <td><%= atm_machine.longitude %></td>
        <td><%= atm_machine.name %></td>
        <td><%= atm_machine.street %></td>
        <td><%= atm_machine.city %></td>
        <td><%= link_to 'Show', atm_machine,class:"btn btn-info"%>
        <td><%= link_to 'Edit', edit_atm_machine_path(atm_machine),class:"btn btn-default" %>
        <td><%= link_to 'Destroy', atm_machine, class:"btn btn-danger",method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Atm machine', new_atm_machine_path,class:"btn btn-primary" %>
